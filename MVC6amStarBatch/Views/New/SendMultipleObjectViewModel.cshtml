@model List<MVC6amStarBatch.Models.EmployeeModel>
@{
    ViewBag.Title = "SendMultipleObjectViewModel";
}

<h2>Send Multiple Object ViewModel</h2>


<table class="table table-condensed table-responsive table-striped">
    <tr>
        <th>EmpId</th>
        <th>EmpName</th>
        <th>EmpSalary</th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.EmpId</td>
            <td>@item.EmpName</td>
            <td>@item.EmpSalary</td>
        </tr>
    }

</table>

