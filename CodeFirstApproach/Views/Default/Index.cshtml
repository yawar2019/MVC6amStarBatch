@model List<CodeFirstApproach.Models.EmployeeModel>
@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<table>
    <tr>
        <th>Id</th>
        <th>Name</th>
        <th>Salary</th>
        <th>Action</th>
    </tr>
    @foreach (var items in Model)
    {
    <tr>
        <td>@items.EmpId</td>
        <td>@items.EmpName</td>
        <td>@items.EmpSalary</td>
        <td>
            @Html.ActionLink("Edit","Edit",new {id=@items.EmpId })|
            @Html.ActionLink("Delete", "Delete", new { id = @items.EmpId })
        </td>
    </tr>
    }
</table>